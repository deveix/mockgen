{
    "entry": [
        "app/**/*.tsx",
        "app/**/*.ts",
        "components/**/*.tsx",
        "components/**/*.ts",
        "lib/**/*.ts",
        "hooks/**/*.ts",
        "hooks/**/*.tsx",
        "stores/**/*.ts"
    ],
    "ignore": [
        "**/*.d.ts",
        "**/*.test.ts",
        "**/*.test.tsx",
        "**/*.spec.ts",
        "**/*.spec.tsx",
        "**/__tests__/**"
    ],
    "ignoreDependencies": [
        "eslint",
        "eslint-config-next"
    ]
}